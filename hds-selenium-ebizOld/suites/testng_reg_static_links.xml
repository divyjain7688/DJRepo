<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- Selenium TestNG XML FILE -->
<suite name="Suite">
	 <listeners>
        <listener class-name="com.selenium.ebiz.listener.TestListener"/>
        <listener class-name="com.selenium.ebiz.listener.AnnotationTransformer"/>
    </listeners>
	 <test name="STATIC LINKS">
		<classes>
			<class name="com.selenium.ebiz.static_links.Error400Page_WORKING" />
			<class name="com.selenium.ebiz.static_links.Error500Page" />
			<class name="com.selenium.ebiz.static_links.PageLevelContextualAlert" />
			<class name="com.selenium.ebiz.static_links.PageLevelGlobalAlert" />
		</classes>
	</test>
</suite>
